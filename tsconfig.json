{
  "compilerOptions": {
    "target": "es6",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "styledSystemMacro/*": ["src/styledSystemMacro/*"]
    },
    "typeRoots": ["./src/types", "node_modules/@types"],
    "outDir": "out"
  },
  "include": ["src/**/*", "src/utils/*"]
}
